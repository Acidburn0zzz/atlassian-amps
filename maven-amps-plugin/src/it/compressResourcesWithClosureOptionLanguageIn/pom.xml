<project>
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.atlassian.amps.it</groupId>
    <artifactId>maven-amps-plugin-compress-resources-test</artifactId>
    <version>testing</version>
    <name>TestCompressResources</name>

    <build>
        <plugins>
            <plugin>
                <groupId>${shitty.groupId}</groupId>
                <artifactId>${shitty.artifactId}</artifactId>
                <version>testing</version>
                <configuration>
                    <closureJsCompiler>true</closureJsCompiler>
                    <closureOptions>
                        <languageIn>ECMASCRIPT5</languageIn>
                    </closureOptions>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>