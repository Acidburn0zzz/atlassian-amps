<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!-- To fix AMPS-1429, MavenGoals#startWebapp configures Cargo to copy this file into Tomcat's conf directory -->
<Context>
    <JarScanner>
        <JarScanFilter
                pluggabilitySkip="${jarsToSkip}"
                tldSkip="${jarsToSkip}"
        />
    </JarScanner>
</Context>
