<project>
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.atlassian.amps.it</groupId>
    <artifactId>amps-maven-plugin-compress-resources-test</artifactId>
    <version>testing</version>
    <name>TestCompressLocalizedResources</name>

    <build>
        <plugins>
            <plugin>
                <groupId>${invoker.groupId}</groupId>
                <artifactId>${invoker.artifactId}</artifactId>
                <version>${project.version}</version>
                <configuration>
                    <encoding>UTF-16</encoding>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>