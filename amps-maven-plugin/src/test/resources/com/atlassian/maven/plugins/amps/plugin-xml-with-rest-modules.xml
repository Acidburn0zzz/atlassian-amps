<atlassian-plugin key="com.atlassian.labs.rest-plugin" name="rest-plugin" plugins-version="2">
    <plugin-info>
        <description>This is the com.atlassian.labs:rest-plugin plugin for Atlassian JIRA.</description>
        <version>1.0.0-SNAPSHOT</version>
        <vendor name="Example Company" url="http://www.example.com/"/>
        <param name="plugin-icon">images/pluginIcon.png</param>
        <param name="plugin-logo">images/pluginLogo.png</param>
    </plugin-info>
    <resource type="i18n" name="i18n" location="rest-plugin"/>
    <web-resource key="rest-plugin-resources" name="rest-plugin Web Resources">
        <dependency>com.atlassian.auiplugin:ajs</dependency>
        <resource type="download" name="rest-plugin.css" location="/css/rest-plugin.css"/>
        <resource type="download" name="rest-plugin.js" location="/js/rest-plugin.js"/>
        <resource type="download" name="images/" location="/images"/>
        <context>rest-plugin</context>
    </web-resource>
    <rest key="bootcampRest" path="/bootcamp" version="1.0">
        <package>com.atlassian.labs</package>
    </rest>
</atlassian-plugin>